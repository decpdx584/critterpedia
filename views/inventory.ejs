<h3>Welcome to your inventory, <%= user.name %></h3>
<h4>You've already got:</h4>
</br>
</br>

<% if (user) { %>
    <% critters.forEach(critter => { %>
        <% belongTos.forEach(belongto => { %>
            <% if (belongto.critterId === critter.id) { %>
                <h5> <%= critter.name.replace(/_/g, " ").toUpperCase() %> </h5>
                <h5> <%= belongto.nickname %> </h5>
                <a class="btn-floating btn-large waves-effect waves-light red"><i class="material-icons">eject</i></a>
                <a class="btn-floating btn-large waves-effect waves-light green"><i class="material-icons">edit</i></a>
                <button type="submit" name="trash">Remove from inventory</button>
                </br>
                </br>
            <% } %>
        <% }) %>
    <% }); %>
<% }; %>